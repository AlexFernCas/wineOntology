    </owl:Class>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#Central_Spain -->

    <owl:Class rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Central_Spain">
