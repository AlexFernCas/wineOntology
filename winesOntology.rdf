    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#Martínez_Rioja -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Martínez_Rioja">
        <rdf:type rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <SpanishWineOntology:hasType rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Rioja_White"/>
        <SpanishWineOntology:hasWinery rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Allende"/>
