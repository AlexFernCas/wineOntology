    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Allende">
        <rdf:type rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Winery"/>
        <SpanishWineOntology:hasRegion rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Briones"/>
