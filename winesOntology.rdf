        <rdfs:comment>Indicates the grape type of the wine.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasRegion -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasRegion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Region"/>
        <rdfs:comment>Indicates the winery&apos;s region</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine_Type"/>
        <rdfs:comment>Type of wine</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasWinery -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasWinery">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#produces"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Winery"/>
        <rdfs:comment>Indicates the winery of wine</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#produces -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#produces">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Winery"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <rdfs:comment>Indicates which wine the winery produces</rdfs:comment>
    </owl:ObjectProperty>
    


