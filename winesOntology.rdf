    </owl:Class>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#La_Rioja -->

    <owl:Class rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#La_Rioja">
