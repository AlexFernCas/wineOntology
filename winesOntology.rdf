    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasRating">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment>Rating of wine</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasYear -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/uoc2023/SpanishWineOntology#hasYear">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/uoc2023/SpanishWineOntology#Wine"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Year when wine was produced</rdfs:comment>
    </owl:DatatypeProperty>
    


